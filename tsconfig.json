{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"experimentalDecorators": true,
		"strict": true,
		"module": "commonjs",
		"esModuleInterop": true,
		"noImplicitAny": true,
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*"]
		},
		"target": "es6",
		"allowJs": true,
		"strictNullChecks": false,
		"skipLibCheck": true,
	},
	"include": [
		"frontend/**/*",
		"backend/**/*", 
		"Utils.ts",
	]
}